{
  "name": "@liveinference/core-sdk",
  "version": "1.0.4",
  "description": "LiveInference SDK for JavaScript",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=18.0.0 <=20.x.x",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "build": "tsup && webpack --mode=production --node-env=production",
    "build:dev": "tsup && webpack --mode=development",
    "build:prod": "tsup && webpack --mode=production --node-env=production",
    "watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doitincloud/live-inference-sdk.git"
  },
  "homepage": "https://github.com/doitincloud/live-inference-sdk",
  "author": "Sam Wen",
  "devDependencies": {
    "@swc/core": "^1.3.95",
    "@types/axios": "^0.14.0",
    "@types/jest": "^29.5.6",
    "@webpack-cli/generators": "^3.0.7",
    "css-loader": "^6.8.1",
    "dotenv": "^16.3.1",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "prettier": "^3.0.3",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "workbox-webpack-plugin": "^7.0.0"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "socket.io-client": "^4.7.2"
  }
}
